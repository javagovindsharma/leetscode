// C++ CODE

    int setBits(int N) {
        // Write Your Code here
        int ans=0;
        while(N){
            if(N&1)ans++;
            N>>=1;
        }
        return ans;
    }

// JAVA CODE

    static int setBits(int N) {
        // code here
        int ans=0;
        while(N!=0){
            if((N&1)==1)ans++;
            N>>=1;
        }
        return ans;
    }
